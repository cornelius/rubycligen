#!/usr/bin/env ruby
#
<%= $license_header %>

require File.expand_path('../../lib/<%= $project_name %>',__FILE__)

Cli.settings = Settings.new

# Set debug flag, so thor throws exceptions on error
ENV["THOR_DEBUG"] = "1"
begin
  Cli.check_unknown_options!
  result = Cli.start ARGV
rescue Thor::Error => e
  STDERR.puts e
  exit 1
end
